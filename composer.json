{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "archtechx/laravel-seo": "^0.9.0",
        "blade-ui-kit/blade-heroicons": "^2.1",
        "doctrine/dbal": "^3.3",
        "embed/embed": "^4.4",
        "filament/filament": "^3.0@beta",
        "flowframe/laravel-drift": "^0.2.0",
        "guzzlehttp/guzzle": "^7.4",
        "http-interop/http-factory-guzzle": "^1.0",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.19",
        "laravel/pint": "^1.10",
        "laravel/tinker": "^2.8",
        "league/commonmark": "^2.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "lorisleiva/lody": "^0.4.0",
        "pirsch-analytics/laravel-pirsch": "^0.4.7",
        "ryangjchandler/blade-capture-directive": "^0.3.0",
        "ryangjchandler/orbit": "^1.2",
        "spatie/image": "^2.2",
        "spatie/laravel-google-fonts": "^1.2",
        "spatie/laravel-image-optimizer": "^1.7",
        "spatie/yaml-front-matter": "^2.0",
        "torchlight/torchlight-commonmark": "^0.5.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.6",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": [
            "pint",
            "npm run prettier"
        ],
        "pint": "pint",
        "test": "phpstan",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
