{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "archtechx/laravel-seo": "^0.7.0",
        "blade-ui-kit/blade-heroicons": "^1.4",
        "doctrine/dbal": "^3.6.2",
        "embed/embed": "^4.4.7",
        "filament/filament": "^2.17.26",
        "filament/forms": "^2.17.26",
        "filament/spatie-laravel-media-library-plugin": "^2.17.26",
        "filament/spatie-laravel-tags-plugin": "^2.17.26",
        "filament/tables": "^2.17.26",
        "flowframe/laravel-previewify": "^0.3.0",
        "graham-campbell/github": "^12.2",
        "jeffgreco13/filament-breezy": "^1.5.3",
        "laravel/framework": "^10.8.0",
        "laravel/horizon": "^5.15",
        "laravel/sanctum": "^3.2.1",
        "laravel/tinker": "^2.8.1",
        "league/commonmark": "^2.4",
        "livewire/livewire": "^2.12.3",
        "pirsch-analytics/laravel-pirsch": "^0.4.6",
        "pxlrbt/filament-spotlight": "^0.3.7",
        "ryangjchandler/blade-capture-directive": "^0.3.0",
        "spatie/laravel-comments": "^1.0",
        "spatie/laravel-comments-livewire": "^1.0",
        "spatie/laravel-feed": "^4.2.1",
        "spatie/laravel-google-fonts": "^1.2.3",
        "spatie/laravel-medialibrary": "^10.8",
        "spatie/laravel-tags": "^4.3.7",
        "torchlight/torchlight-commonmark": "^0.5.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21.0",
        "laravel/pint": "^1.8",
        "laravel/sail": "^1.21.4",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.5",
        "phpunit/phpunit": "^10.1.1",
        "spatie/laravel-ignition": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ]
}
