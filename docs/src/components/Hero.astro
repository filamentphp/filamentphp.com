---
import book from '@assets/hero/book.webp'
import bulbLight from '@assets/hero/bulb-light.webp'
import bulbDark from '@assets/hero/bulb-dark.webp'

const { getStartedLink } = Astro.props
---

<section class="py-5 sm:py-10 px-5">
    <div class="text-center">
        <div
            class="pt-12 text-3xl sm:text-4xl lg:text-5xl font-black text-midnight dark:text-white"
        >
            Filament Documentation
        </div>
        <div
            class="pt-6 text-base sm:text-lg md:text-xl lg:text-2xl text-evening/60 dark:text-white/80"
        >
            A collection of beautiful full-stack components.
            <br />
            The perfect starting point for your next app.
        </div>
    </div>
    <div class="grid place-items-center pt-6">
        <a
            href={getStartedLink}
            class="px-7 py-3 rounded-full transition duration-300 text-white bg-[#ffbc74] hover:bg-[#ffbc74]/80 dark:text-midnight dark:hover:bg-[#ffbc74]/90 relative z-30"
        >
            Get Started
        </a>
    </div>
    <div class="pt-12 relative w-full max-w-7xl mx-auto min-h-[270px]">
        <div class="grid place-items-center">
            <div class="grid relative w-80 h-56">
                <!-- Bulb Light -->
                <img
                    src={bulbLight.src}
                    alt=""
                    class="[grid-area:1/-1] self-start relative -left-5 justify-self-center w-64 dark:hidden z-10"
                />
                <!-- Bulb Dark -->
                <img
                    src={bulbDark.src}
                    alt=""
                    class="[grid-area:1/-1] self-start mix-blend-lighten relative -left-1 -top-10 justify-self-center w-40 -z-10 hidden dark:block"
                />
                <!-- Book -->
                <img
                    src={book.src}
                    alt=""
                    class="[grid-area:1/-1] self-end justify-self-center w-64"
                />
            </div>
        </div>
